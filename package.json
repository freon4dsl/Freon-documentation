{
	"name": "FreonDocumentation",
	"version": "0.7.0",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/freon4dsl/Freon-documentation"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ./src",
		"link-check": "find src/routes -name '*.md' -exec yarn markdown-link-check {};",
		"embedme": "embedme --source-root code-examples \"src/routes/**/*.md\""
	},
	"devDependencies": {
		"@freon4dsl/core": "0.7.0-beta4",
		"@freon4dsl/core-svelte": "0.7.0-beta4",
		"@freon4dsl/meta": "0.7.0-beta4",
		"@prettier/sync": "^0.5.2",
		"@smui/button": "^7.0.0",
		"@smui/drawer": "^7.0.0",
		"@smui/list": "^7.0.0",
		"@smui/tooltip": "^7.0.0",
		"@sveltejs/adapter-static": "^3.0.4",
		"@sveltejs/kit": "^2.7.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^9.6.0",
		"@types/cookie": "^0.6.0",
		"@types/node": "^22.3.0",
		"embedme": "^1.22.1",
		"eslint": "^9.0.0",
		"eslint-plugin-svelte": "^2.36.0",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.0.0",
		"mdsvex": "^0.12.2",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.5",
		"svelte-preprocess": "^6.0.2",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"mdast-util-to-string": "^4.0.0",
		"unist-util-visit": "^5.0.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^5.0.13",
		"svelte-intersection-observer": "^1.0.0",
		"@lukeed/uuid": "^2.0.1"
	}
}
