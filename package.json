{
  "name": "projectItDocumentation",
  "version": "0.3.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joswarmer/ProjectItDocumentation.git"
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "link-check": "find src/routes -name '*.md' -exec yarn markdown-link-check {};",
    "prepare": "cd scripts && yarn checkLinks && yarn navTree && cd .. && yarn embedme --source-root code-example/src \"src/routes/**/*.md\""
  },
  "devDependencies": {
    "@smui/button": "^5.0.0-beta.7",
    "@smui/drawer": "^5.0.0-beta.7",
    "@smui/list": "^5.0.0-beta.7",
    "@smui/tooltip": "^5.0.0-beta.7",
    "@sveltejs/adapter-static": "^1.0.0-next.28",
    "@sveltejs/kit": "1.0.0-next.266",
    "@types/cookie": "^0.4.1",
    "embedme": "^1.22.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.34.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "mdsvex": "^0.9.8",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "@lukeed/uuid": "^2.0.0"
  }
}
