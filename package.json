{
	"name": "FreonDocumentation",
	"version": "0.7.0",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/freon4dsl/Freon-documentation"
	},
	"scripts": {
		"buildSite": "cd ./scripts && npm run buildSite && cd ../ && npm run format",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --log-level silent --write ./src",
		"formatMd": "prettier --log-level silent --write ./docu-content"
	},
	"devDependencies": {
		"@freon4dsl/core": "1.0.0-beta1",
		"@freon4dsl/core-svelte": "1.0.0-beta1",
		"@freon4dsl/meta": "1.0.0-beta1",
		"@freon4dsl/webapp-lib": "^1.0.0-beta1",
		"@prettier/sync": "^0.5.2",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.8.0",
		"@sveltejs/vite-plugin-svelte": "3.1.2",
		"@types/eslint": "^9.6.1",
		"@types/cookie": "^1.0.0",
		"@types/node": "^22.9.0",
		"embedme": "^1.22.1",
		"eslint": "^9.14.0",
		"eslint-plugin-svelte": "^2.46.0",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.12.0",
		"mdsvex": "^0.12.3",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"svelte": "4.2.19",
		"svelte-check": "^4.0.6",
		"svelte-preprocess": "^6.0.3",
		"tslib": "^2.8.1",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.13.0",
		"vite": "^5.4.10",
		"mdast-util-to-string": "^4.0.0",
		"unist-util-visit": "^5.0.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^5.1.0",
		"svelte-intersection-observer": "^1.0.0",
		"@lukeed/uuid": "^2.0.1"
	}
}
