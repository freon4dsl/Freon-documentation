<+>/* Original by Taylor Buckner, based on [VS Codes Dark+](https://github.com/VS Codes Dark+) */\n:root {\n\t/* all colors are put in variables, to be able to switch from light to dark theme */\n\t--code-color-grey: #d4d4d4;\n\t--code-color-purple: #c586c0;\n\t--code-color-light-marroon: #ce9178;\n\t--code-color-blue: #569cd6;\n\t--code-color-light-blue: #9cdcfe;\n\t--code-color-light-green: #b5cea8;\n\t--code-color-dark-green: #6a9955;\n\t--code-color-red: #db4c69;\n\t--code-color-grey-green: #dcdcaa;\n\t--code-color-light-red: #d16969;\n\t--code-color-aqua: #4ec9b0;\n\t--code-color-black: #312700;\n\t--code-color-dark-blue: #264f78;\n\t--code-color-beige: #d7ba7d;\n\t--code-color-dark-grey: #808080;\n\t--code-color-light-yellow: #f7ebc6;\n\t--code-color-dark-yellow: #f7d87c;\n}\n\nbody {\n\t/* The default light settings */\n\t--code-color: var(--code-color-black);\n\t--code-background1: var(--code-color-dark-grey);\n\t--code-background: #fdfdfd;\n\t--token-operator: #6b4d2d;\n\t--token-not-pre: #c92c2c;\n\t--token-constant: var(--code-color-light-blue);\n\t--token-comment: #7D8B99;\n\t--token-number: #c92c2c;\n\t--token-string: #2f9c0a;\n\t--token-keyword: #1990b8;\n\t--token-function: #1b38c4;\n\t--token-regex: var(--code-color-light-red);\n\t--token-classname: #237009;\n\t--token-selector: #2f9c0a;\n\t--token-punctuation: #5F6364;\n\t--line-highlight: var(--code-color-dark-yellow);\n\t--line-highlight-shadow: var(--code-color-light-yellow);\n}\n\nbody.dark {\n\t/* The dark settings */\n\t--code-color: var(--code-color-grey);\n\t--code-background1: var(--code-color-dark-blue);\n\t--code-background: var(--code-color-black);\n\t--token-operator: var(--code-color-blue);\n\t--token-not-pre: var(--code-color-red);\n\t--token-constant: var(--code-color-light-blue);\n\t--token-comment: var(--code-color-dark-green);\n\t--token-number: var(--code-color-light-green);\n\t--token-string: var(--code-color-light-marroon);\n\t--token-keyword: var(--code-color-purple);\n\t--token-function: var(--code-color-grey-green);\n\t--token-regex: var(--code-color-light-red);\n\t--token-classname: var(--code-color-aqua);\n\t--token-selector: var(--code-color-beige);\n\t--token-punctuation: var(--code-color-dark-grey);\n\t--line-highlight: var(--code-color-light-yellow);\n\t--line-highlight-shadow: var(--code-color-dark-yellow);\n\t--code-inline-background: var(--code-color-dark-grey);\n}\n\n/* code blocks within a paragraph need to stand out more */\np code {\n\tbackground-color: var(--code-inline-background);\n}\n\npre[class*='language-'],\ncode[class*='language-'] {\n\tcolor: var(--code-color);\n\tfont-size: 13px;\n\ttext-shadow: none;\n\tfont-family: Menlo, Monaco, Consolas, 'Andale Mono', 'Ubuntu Mono', 'Courier New', monospace;\n\tdirection: ltr;\n\ttext-align: left;\n\twhite-space: pre;\n\tword-spacing: normal;\n\tword-break: normal;\n\tline-height: 1.5;\n\t-moz-tab-size: 4;\n\t-o-tab-size: 4;\n\ttab-size: 4;\n\t-webkit-hyphens: none;\n\t-moz-hyphens: none;\n\t-ms-hyphens: none;\n\thyphens: none;\n}\n\npre[class*='language-']::selection,\ncode[class*='language-']::selection,\npre[class*='language-'] *::selection,\ncode[class*='language-'] *::selection {\n\ttext-shadow: none;\n\tbackground: var(--code-background1);\n}\n\n@media print {\n\tpre[class*='language-'],\n\tcode[class*='language-'] {\n\t\ttext-shadow: none;\n\t}\n}\n\npre[class*='language-'] {\n\tpadding: 1em;\n\tmargin: 0.5em 0;\n\toverflow: auto;\n\tbackground: var(--code-background);\n}\n\n:not(pre) > code[class*='language-'] {\n\tpadding: 0.1em 0.3em;\n\tborder-radius: 0.3em;\n\tcolor: var(--token-not-pre);\n\tbackground: var(--code-background);\n}\n/*********************************************************\n* Tokens\n*/\n.namespace {\n\topacity: 0.7;\n}\n\n.token.doctype .token.doctype-tag {\n\tcolor: var(--token-operator);\n}\n\n.token.doctype .token.name {\n\tcolor: var(--token-constant);\n}\n\n.token.comment,\n.token.prolog {\n\tcolor: var(--token-comment);\n}\n\n.token.punctuation,\n.language-html .language-css .token.punctuation,\n.language-html .language-javascript .token.punctuation {\n\tcolor: var(--code-color);\n}\n\n.token.property,\n.token.tag,\n.token.boolean,\n.token.number,\n.token.constant,\n.token.symbol,\n.token.inserted,\n.token.unit {\n\tcolor: var(--token-number);\n}\n\n.token.selector,\n.token.attr-name,\n.token.string,\n.token.char,\n.token.builtin,\n.token.deleted {\n\tcolor: var(--token-string);\n}\n\n.language-css .token.string.url {\n\ttext-decoration: underline;\n}\n\n.token.operator,\n.token.entity {\n\tcolor: var(--code-color);\n}\n\n.token.operator.arrow {\n\tcolor: var(--token-operator);\n}\n\n.token.atrule {\n\tcolor: var(--token-string);\n}\n\n.token.atrule .token.rule {\n\tcolor: var(--token-keyword);\n}\n\n.token.atrule .token.url {\n\tcolor: var(--token-constant);\n}\n\n.token.atrule .token.url .token.function {\n\tcolor: var(--token-function);\n}\n\n.token.atrule .token.url .token.punctuation {\n\tcolor: var(--code-color);\n}\n\n.token.keyword {\n\tcolor: var(--token-operator);\n}\n\n.token.keyword.module,\n.token.keyword.control-flow {\n\tcolor: var(--token-keyword);\n}\n\n.token.function,\n.token.function .token.maybe-class-name {\n\tcolor: var(--token-function);\n}\n\n.token.regex {\n\tcolor: var(--token-regex);\n}\n\n.token.important {\n\tcolor: var(--token-operator);\n}\n\n.token.italic {\n\tfont-style: italic;\n}\n\n.token.constant {\n\tcolor: var(--token-constant);\n}\n\n.token.class-name,\n.token.maybe-class-name {\n\tcolor: var(--token-classname);\n}\n\n.token.console {\n\tcolor: var(--token-constant);\n}\n\n.token.parameter {\n\tcolor: var(--token-constant);\n}\n\n.token.interpolation {\n\tcolor: var(--token-constant);\n}\n\n.token.punctuation.interpolation-punctuation {\n\tcolor: var(--token-operator);\n}\n\n.token.boolean {\n\tcolor: var(--token-operator);\n}\n\n.token.property,\n.token.variable,\n.token.imports .token.maybe-class-name,\n.token.exports .token.maybe-class-name {\n\tcolor: var(--token-constant);\n}\n\n.token.selector {\n\tcolor: var(--token-selector);\n}\n\n.token.escape {\n\tcolor: var(--token-selector);\n}\n\n.token.tag {\n\tcolor: var(--token-operator);\n}\n\n.token.tag .token.punctuation {\n\tcolor: var(--token-punctuation);\n}\n\n.token.cdata {\n\tcolor: var(--token-punctuation);\n}\n\n.token.attr-name {\n\tcolor: var(--token-constant);\n}\n\n.token.attr-value,\n.token.attr-value .token.punctuation {\n\tcolor: var(--token-string);\n}\n\n.token.attr-value .token.punctuation.attr-equals {\n\tcolor: var(--code-color);\n}\n\n.token.entity {\n\tcolor: var(--token-operator);\n}\n\n.token.namespace {\n\tcolor: var(--token-classname);\n}\n/*********************************************************\n* Language Specific\n*/\n\npre[class*='language-javascript'],\ncode[class*='language-javascript'],\npre[class*='language-jsx'],\ncode[class*='language-jsx'],\npre[class*='language-typescript'],\ncode[class*='language-typescript'],\npre[class*='language-tsx'],\ncode[class*='language-tsx'] {\n\tcolor: var(--token-constant);\n}\n\npre[class*='language-css'],\ncode[class*='language-css'] {\n\tcolor: var(--token-string);\n}\n\npre[class*='language-html'],\ncode[class*='language-html'] {\n\tcolor: var(--code-color);\n}\n\n.language-regex .token.anchor {\n\tcolor: var(--token-function);\n}\n\n.language-html .token.punctuation {\n\tcolor: var(--token-punctuation);\n}\n/*********************************************************\n* Line highlighting\n*/\npre[class*='language-'] > code[class*='language-'] {\n\tposition: relative;\n\tz-index: 1;\n}\n\n.line-highlight.line-highlight {\n\tbackground: var(--line-highlight);\n\tbox-shadow: inset 5px 0 0 var(--line-highlight-shadow);\n\tz-index: 0;\n}\n
