{
	"private": true,
	"name": "navtree",
	"type": "module",
	"bin": {
		"navTree": "dist/NavTreeFiller.js"
	},
	"scripts": {
		"generateExamples": "npm run docuProject && npm run tutorial",
		"docuProject": "cd ../code-examples/Insurance && npm run generate && cd ../../scripts/",
		"tutorial": "cd ../code-examples/CustomizationsProject && npm run generate && cd ../../scripts/",
		"typerExample": "cd ../code-examples/TyperExample && npm run generate && cd ../../scripts/",
		"embedme": "embedme --source-root ../code-examples \"../docu-content/**/*.md\"",
		"embedExamples": "npm run generateExamples && npm run embedme",
		"install-examples": "tsc && node dist/InstallCodeExamples.js",
		"md2svelte": "tsc && node dist/Generator.js",
		"buildSite": "npm run install-examples && npm run embedExamples && npm run md2svelte",
		"install-local": "npm install --registry http://localhost:4873"
	},
	"devDependencies": {
		"@types/prismjs": "1.26.5"
	},
	"dependencies": {
		"prismjs": "1.29.0"
	}
}
